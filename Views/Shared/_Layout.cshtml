@inject Microsoft.AspNetCore.Identity.UserManager<User> SignInManager
<!DOCTYPE html>
<html lang="ar" dir="rtl">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - CourtHouse</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link href="~/bootstrap-icons/font/bootstrap-icons.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/css/site.css" />
    <link href="~/css/Chart.css" rel="stylesheet" />
</head>
<body>
    <header>
        <partial name="_PartialNav" />
    </header>
    <div class="container-fluid" style="background-image: url('/img/bg3.jpg'); background-size:100% 100%; background-repeat: no-repeat; background-attachment: fixed; height: 77vh; width: 100%;">
        <main role="main">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2021 - CourtHouse
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/js/Chart.js"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
