@model CourtHouse.Models.Realty
<div class="modal fade" id="ShowPopup">
    <div class="modal-dialog modal-xl">
        <div class="modal-content">
            <form asp-action="AddRealty">
                <div class="modal-header">
                    <h2>الرجاء إدخال رقم العقار</h2>
                </div>
                <div class="modal-body">
                    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                    <div class="form-group">
                        <label class="control-label">رقم العقار</label>
                        <input  asp-for="id" class="form-control"/>
                    </div>
                </div>
                <div class="modal-footer">
                    <input type="submit" value="بحث" class="btn btn-info"/>
                </div>
            </form>
        </div>
    </div>
</div>



